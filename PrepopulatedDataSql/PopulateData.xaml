<Window x:Class="PrepopulatedDataSql.PopulateData"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PrepopulatedDataSql"
        mc:Ignorable="d"
        Title="PopulateData" Height="600" Width="400">
    <Grid>

        <TextBlock x:Name="firstNameblock" Margin="128,77,128,0" TextWrapping="Wrap"  VerticalAlignment="Top" Height="43" Width="144" Background="#FFFFDADA"/>

        <TextBlock x:Name="lastNameblock" HorizontalAlignment="Center" Margin="0,172,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Height="43" Width="144" Background="#FFFFDADA"/>
        <TextBlock  x:Name="emailblock" HorizontalAlignment="Center" Margin="0,279,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Height="43" Width="144" Background="#FFFFDADA"/>
        <TextBlock  x:Name="passwordBlock" HorizontalAlignment="Center" Margin="0,375,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="43" Width="144" Background="#FFFFDADA"/>
        <Button Content="Populate User" Margin="142,492,142,45" Click="Button_Click"/>



    </Grid>
</Window>
